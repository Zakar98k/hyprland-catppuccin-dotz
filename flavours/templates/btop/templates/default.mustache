
# Main background, empty for terminal default, need to be empty if you want transparent background
theme[main_bg]="#{{base00-hex}}"

# Main text color
theme[main_fg]="#{{base05-hex}}"

# Title color for boxes
theme[title]="#{{base05-hex}}"

# Higlight color for keyboard shortcuts
theme[hi_fg]="#{{base08-hex}}"

# Background color of selected items
theme[selected_bg]="#{{base0A-hex}}"

# Foreground color of selected items
theme[selected_fg]="#{{base00-hex}}"

# Color of inactive/disabled text
theme[inactive_fg]="#{{base01-hex}}"

# Color of text appearing on top of graphs, i.e uptime and current network graph scaling
theme[graph_text]="#{{base0A-hex}}"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#{{base0D-hex}}"

# Cpu box outline color
theme[cpu_box]="#{{base0A-hex}}"

# Memory/disks box outline color
theme[mem_box]="#{{base0B-hex}}"

# Net up/down box outline color
theme[net_box]="#{{base0C-hex}}"

# Processes box outline color
theme[proc_box]="#{{base0D-hex}}"

# Box divider line and small boxes line color
theme[div_line]="#{{base0E-hex}}"

# Temperature graph colors
theme[temp_start]="#{{base0C-hex}}"
theme[temp_mid]="#{{base09-hex}}"
theme[temp_end]="#{{base08-hex}}"

# CPU graph colors
theme[cpu_start]="#{{base0B-hex}}"
theme[cpu_mid]="#{{base09-hex}}"
theme[cpu_end]="#{{base08-hex}}"

# Mem/Disk free meter
theme[free_start]="#{{base0B-hex}}"
theme[free_mid]=""
theme[free_end]=""

# Mem/Disk cached meter
theme[cached_start]="#{{base0C}}"
theme[cached_mid]=""
theme[cached_end]=""

# Mem/Disk available meter
theme[available_start]="#{{base0A-hex}}"
theme[available_mid]=""
theme[available_end]=""

# Mem/Disk used meter
theme[used_start]="#{{base08-hex}}"
theme[used_mid]=""
theme[used_end]=""

# Download graph colors
theme[download_start]="#{{base09-hex}}"
theme[download_mid]=""
theme[download_end]=""

# Upload graph colors
theme[upload_start]="#{{base0E-hex}}"
